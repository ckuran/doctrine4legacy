{
    "name": "ckuran/doctrine4legacy",
    "description": "Simple Doctrine wrapper for legacy apps.",
    "keywords": ["doctrine","wrapper"],
    "homepage": "https://github.com/ckuran/doctrine4legacy",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ckuran",
            "email": "c@kuran.pl"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^5.6 || ^7.0 || ^7.1 || ^7.2",
        "beberlei/doctrineextensions": "^1.1 || ^1.2",
        "doctrine/orm": "^2.5 || ^2.6 || ^2.7",
        "symfony/dotenv": "^3.4 || ^4.4 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "ckuran\\":"src/"
        }
    }
}
